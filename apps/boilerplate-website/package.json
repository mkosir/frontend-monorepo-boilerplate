{
  "name": "boilerplate-website",
  "version": "0.0.0",
  "scripts": {
    "build": "next build",
    "dev": "open http://localhost:3101 && next dev --port 3101",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "test-coverage": "jest --coverage --verbose",
    "storybook": "storybook dev -p 6101 --quiet",
    "storybook-build": "storybook build -o build-storybook",
    "update-dependencies": "npx npm-check-updates -u"
  },
  "dependencies": {
    "config-mui": "*",
    "next": "14.1.1",
    "ui": "*",
    "ui-mui": "*",
    "utils-mui": "*",
    "utils-version": "*"
  },
  "devDependencies": {
    "config-storybook": "*",
    "config-tsconfig": "*",
    "utils-test": "*"
  }
}
